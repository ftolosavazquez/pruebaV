{
  "name": "veryfi/veryfi-php",
  "description": "veryfi sdk for php",
  "type": "library",
  "keywords": ["ocr", "veryfi", "data processing"],
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Sebastian Carmona",
      "email": "sebastiancarmona@veryfi.com"
    },
    {
      "name": "Alejandro Uribe",
      "email": "alejandro@veryfi.com"
    }
  ],

  "require": {
    "php": ">=8.0.0",
    "ext-curl": "*"
  },

  "require-dev": {
    "phpunit/phpunit": "^9"
  },

  "autoload": {
    "psr-4": {
      "Veryfi\\": "src/"
    },
    "files": [
      "src/Client.php"
    ]
  },

  "autoload-dev": {
    "psr-4": {
      "Veryfi\\": "src/"
    },
    "files": [
      "src/Client.php"
    ]
  },

  "config": {
    "sort-packages": true
  }
}
